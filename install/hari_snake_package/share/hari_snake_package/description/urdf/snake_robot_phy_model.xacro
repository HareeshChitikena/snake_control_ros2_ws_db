        </geometry>
      </collision>
      <xacro:inertial_box mass="${link_mass}" x="${link_length}" y="${link_width}" z="${link_height}">
      	    <origin xyz="${next_link_with_gap} 0 0" rpy="0 0 0" />
      </xacro:inertial_box>
    </link>
          
    <joint name="joint_2" type="revolute">
